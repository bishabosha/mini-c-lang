int a, b, c, d;
int foo(void) {
    a = 10, b = 50, c = b, d = 20;
}
int main(void) {
    a;
    return 0;
}

List(
    Declaration(auto,int,Identifier(a)), 
    Declaration(auto,int,Identifier(b)), 
    Declaration(auto,int,Identifier(c)), 
    Declaration(auto,int,Identifier(d)), 
    Function(
        auto,
        int,
        Identifier(foo),
        LVoid,
        List(
            Assignment(Identifier(a),Constant(10)),
            Assignment(Identifier(b),Constant(50)),
            Assignment(Identifier(c),Identifier(b)),
            Assignment(Identifier(d),Constant(20))))), 
    Function(
        auto,
        int,
        Identifier(main),
        LVoid,
        List(
            Identifier(a), 
            Return(Some(Constant(0)))))))