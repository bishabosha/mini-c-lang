int main(void) {
    int a, b;
    a = read_int();
    b = read_int();
    if (a < b) {
        if (a > 0) {
            print_int(a * b);
        } else if (b < 0) {
            print_int(a + b);
        } else {
            print_int(909);
        }
    } else {
        print_int(a % b);
    }
    return 0;
}